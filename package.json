{
  "name": "meeting-notes",
  "displayName": "meeting_notes",
  "publisher": "IsaacMcAuley",
  "description": "A tool for to making meeting notes easier",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.73.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/imcauley/MeetingNotes"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:meeting-notes.addToTodo",
    "onCommand:meeting-notes.addToAgenda",
    "onCommand:meeting-notes.addToNotes"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "meeting-notes.addToTodo",
        "title": "Add To Todo"
      },
      {
        "command": "meeting-notes.addToAgenda",
        "title": "Add To Agenda"
      },
      {
        "command": "meeting-notes.addToNotes",
        "title": "Add To Notes"
      }
    ],
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/snippet.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}